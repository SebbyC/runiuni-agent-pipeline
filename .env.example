# RuniUni Event Pipeline Environment Variables

# API Keys
OPENAI_API_KEY="your_openai_api_key_here"
GOOGLE_API_KEY="your_google_api_key_here" 
SEARCH_ENGINE_ID="your_google_custom_search_engine_id_here"
GOOGLE_PLACES_API_KEY="your_google_places_api_key_here"

# RuniUni Credentials
RUNIUNI_USERNAME="your_runiuni_username"
RUNIUNI_PASSWORD="your_runiuni_password"
RUNIUNI_BASE_URL="https://api.runiuni.com"

# Optional Configuration
# LOG_LEVEL="INFO"  # DEBUG, INFO, WARNING, ERROR, CRITICAL
# DEFAULT_EVENT_LIMIT=5  # Maximum events to retrieve per location
# IMAGE_BATCH_SIZE=5  # Number of events to process in each image batch
# REQUEST_DELAY=1  # Delay in seconds between API requests
# SAVE_FILES=true  # Whether to save intermediate JSON files